{
  "schemaVersion": 1,
  "id": "mod_id",
  "version": "${version}",
  "name": "Mod Name",
  "description": "Mod Description",
  "authors": [
    "Author"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://example.com/",
    "sources": "https://example.com/"
  },
  "license": "The Unlicense",
  "icon": "assets/mod_name/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.mod_author.mod_id.ModName"
      }
    ]
  },
  "mixins": [
    "mod_id.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}"
  }
}